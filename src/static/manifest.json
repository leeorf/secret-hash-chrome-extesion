{
  "manifest_version": 3,
  "name": "Secret Hash",
  "version": "1.0.0",
  "description": "Chrome extension to secretly hash texts.",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "Secret Hash",
    "default_icon": "icon.png"
  },
  "options_page": "options.html",
  "permissions": ["storage"]
}
